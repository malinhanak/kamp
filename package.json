{
	"name": "kamp",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://malinhanak.github.io/kamp/",
	"dependencies": {
		"@fontsource/poppins": "^4.3.0",
		"@fontsource/roboto-condensed": "^4.3.0",
		"@fontsource/roboto-mono": "^4.3.0",
		"@testing-library/jest-dom": "^5.12.0",
		"@testing-library/react": "^11.2.6",
		"@testing-library/user-event": "^13.1.8",
		"connected-react-router": "^6.9.1",
		"date-fns": "^2.21.3",
		"dom-testing-library": "^5.0.0",
		"dotenv": "^8.2.0",
		"firebase": "^8.4.2",
		"firebase-tools": "9.12.1",
		"framer-motion": "^4.1.17",
		"history": "4.10.1",
		"htmr": "^1.0.0",
		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-feather": "^2.0.9",
		"react-helmet-async": "^1.0.9",
		"react-redux": "^7.2.4",
		"react-redux-firebase": "^3.10.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "4.0.3",
		"react-toast-notifications": "^2.4.4",
		"react-use": "^17.2.4",
		"redux": "^4.1.0",
		"redux-auth-wrapper": "^3.0.0",
		"redux-firestore": "^0.15.0",
		"reselect": "^4.0.0",
		"styled-components": "^5.2.3",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"test:nowatch": "react-scripts test ./src/components/__tests__ --watchAll=false",
		"format": "yarn prettier --write",
		"eject": "react-scripts eject",
		"prepare": "husky install"
	},
	"jest": {
		"modulePathIgnorePatterns": [
			"./src/__tests__/firebase*"
		]
	},
	"eslintConfig": {
		"ignorePatterns": [
			"functions"
		],
		"plugins": [
			"unused-imports"
		],
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"rules": {
			"no-unused-vars": "off",
			"unused-imports/no-unused-imports": "error",
			"unused-imports/no-unused-vars": "warn"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged  --allow-empty",
			"pre-push": "yarn test:nowatch"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"yarn format"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@firebase/rules-unit-testing": "^1.3.7",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-unused-imports": "^1.1.1",
		"husky": "^6.0.0",
		"uuidv4": "^6.2.8"
	}
}
